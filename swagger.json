						"_id": {
							"type": "string",
							"description": "Muscle id"
						},
						"filter": {
							"type": "string",
							"description": "Filter by muscles"
						},
						"name": {
							"type": "string",
							"description": "Muscle name"
						},
						"imgURL": {
							"type": "string",
							"description": "Image URL"
						}
					}
				}
			},
			"ExercisesEquipmentsResponse": {
				"type": "array",
